doi = {10.1038/35101614},
file = {:Users/nolson/Library/Application Support/Mendeley Desktop/Downloaded/McClelland et al. - 2001 - Complete genome sequence of Salmonella enterica serovar Typhimurium LT2.pdf:pdf},
