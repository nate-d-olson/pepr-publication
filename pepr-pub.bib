@article{Koren2013,
abstract = {BACKGROUND: The short reads output by first- and second-generation DNA sequencing instruments cannot completely reconstruct microbial chromosomes. Therefore, most genomes have been left unfinished due to the significant resources required to manually close gaps in draft assemblies. Third-generation, single-molecule sequencing addresses this problem by greatly increasing sequencing read length, which simplifies the assembly problem.$\backslash$n$\backslash$nRESULTS: To measure the benefit of single-molecule sequencing on microbial genome assembly, we sequenced and assembled the genomes of six bacteria and analyzed the repeat complexity of 2,267 complete bacteria and archaea. Our results indicate that the majority of known bacterial and archaeal genomes can be assembled without gaps, at finished-grade quality, using a single PacBio RS sequencing library. These single-library assemblies are also more accurate than typical short-read assemblies and hybrid assemblies of short and long reads.$\backslash$n$\backslash$nCONCLUSIONS: Automated assembly of long, single-molecule sequencing data reduces the cost of microbial finishing to \$1,000 for most genomes, and future advances in this technology are expected to drive the cost lower. This is expected to increase the number of completed genomes, improve the quality of microbial genome databases, and enable high-fidelity, population-scale studies of pan-genomes and chromosomal organization.},
archivePrefix = {arXiv},
arxivId = {1304.3752},
author = {Koren, Sergey and Harhay, Gregory P and Smith, Timothy P L and Bono, James L and Harhay, Dayna M and Mcvey, Scott D and Radune, Diana and Bergman, Nicholas H and Phillippy, Adam M},
doi = {10.1186/gb-2013-14-9-r101},
eprint = {1304.3752},
file = {:Users/nolson/Desktop/add\_to\_mendeley/gb-2013-14-9-r101.pdf:pdf},
isbn = {1465-6906},
issn = {1465-6914},
journal = {Genome biology},
keywords = {14,2013,9,com,en et al,genome biology 2013,genomebiology,http,r101},
number = {9},
pages = {R101},
pmid = {24034426},
title = {{Reducing assembly complexity of microbial genomes with single-molecule sequencing.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4053942\&tool=pmcentrez\&rendertype=abstract},
volume = {14},
year = {2013}
}

